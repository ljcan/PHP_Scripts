        <title>JavaScript之object对象类型</title>
        <meta name="author" content="Shinelon" />
        <!-- Date: 2017-07-04 -->
        <script type="text/javascript" charset="UTF-8">
            //创建对象的两种方式
            var obj=new object();
            var obj={};
            //创建对象属性
            obj.name="张三";
            obj.age=20;
            @obj.say=function(){
                alert('hello world');
            }
            alert(obj.name);
            alert(obj.age);
            obj.say();
        </script>
    </head>
    <body>

    </body>
