        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>PHP中microtime函数测试</title>
        <meta name="author" content="Shinelon" />
        <!-- Date: 2017-07-03 -->
    </head>
    <body>
	<?php
	$a=1;
	$starttime=microtime();     //获取开始懂得时间
	for($i=0;$i<2000;$i++){
		$a++;
	}
	$endtime=microtime();        //获取结束的时间
	echo "执行的时间是:".($endtime-$starttime);
	?>
    </body>
</html>

